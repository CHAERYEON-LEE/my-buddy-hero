{
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src",
    "@svgs": "${workspaceRoot}/public/svgs",
    "@images": "${workspaceRoot}/public/images"
  },
  "postcss.validate": false,
  "files.associations": {
    "*.css": "postcss"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  }
}
