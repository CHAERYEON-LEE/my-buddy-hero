{
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src",
    "@public": "${workspaceRoot}/public"
  },
  "postcss.validate": false,
  "files.associations": {
    "*.css": "postcss"
  },
  "emmet.includeLanguages": {
    "postcss": "css"
  }
}
